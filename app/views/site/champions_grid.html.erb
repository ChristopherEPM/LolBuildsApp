<article class="main-wrapper">
  <div class="champions-grid">
    
      <% @champions.sort_by!{|champ| champ.name }.each do | champion | %>
        <a href=<%= champion_path(champion.id) %>><img class="champions-grid-main" src=<%="http://ddragon.leagueoflegends.com/cdn/6.5.1/img/champion/#{champion.image['full']}" %> alt="Link to <%= champion.name%> profile" title="<%="#{champion.name}, #{champion.title}" %>" > </a>
      <% end %>

  </div>
</article>